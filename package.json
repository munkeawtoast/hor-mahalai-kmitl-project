{
  "name": "turborepo-vue-starter",
  "author": "Arneesh Aima",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "config/*",
    "lib/helpers/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{vue,js,ts,tsx,md}\""
  },
  "devDependencies": {
    "prettier": "^2.5.1",
    "turbo": "latest"
  },
  "engines": {
    "npm": ">=7.0.0",
    "node": ">=14.0.0"
  },
  "packageManager": "yarn@1.22.15"
}
