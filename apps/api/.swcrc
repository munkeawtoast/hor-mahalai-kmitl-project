{
  "$schema": "https://json.schemastore.org/swcrc",

  "isModule": true,
  "module": {
    "type": "es6",
    "noInterop": false,
    "strictMode": true
  },
  "jsc": {
    "parser": {
      "syntax": "typescript",
    },
    "target": "es2022",
    "loose": false,
    "externalHelpers": false,
    "paths": {
      "~utils": ["./src/utils"],
      "~utils/*": ["./src/utils/*"]
    }
  },
  "minify": false
}